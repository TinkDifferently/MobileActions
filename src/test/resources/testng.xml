            <class name="ru.vtb.app.chat.tests.light.Buttons_Emoji"/>
            <class name="ru.vtb.app.chat.tests.light.Buttons_Cursive"/>
            <class name="ru.vtb.app.chat.tests.light.Buttons_SpecialSymbols"/>
            <class name="ru.vtb.app.chat.tests.light.Buttons_ManyManyButtons"/>
        </classes>
    </test>
    <test name="Мультизагрузка" enabled="false">
        <parameter name="authStyle" value="Авторизованная зона"/>
        <parameter name="clientType" value=""/>
        <classes>
            <class name="ru.vtb.app.chat.tests.MultiLoadTest_MaximumFiles"/>
            <class name="ru.vtb.app.chat.tests.MultiLoadTest_AddingFiles"/>
            <class name="ru.vtb.app.chat.tests.MultiLoadTest_IncorrectFiles"/>
            <class name="ru.vtb.app.chat.tests.MultiLoadTest_TooMuchFiles"/>
        </classes>
    </test>
    <test name="Debug">
        <parameter name="authStyle" value="Авторизованная зона"/>
        <parameter name="clientType" value=""/>
        <classes>
            <class name="ru.vtb.app.chat.tests.EmojiTest"/>
            <class name="ru.vtb.app.chat.tests.FastButtonsTest_Hi"/>
        </classes>
    </test>
    <test name="Авторизованная зона" enabled="false">
        <parameter name="authStyle" value="Авторизованная зона"/>
        <parameter name="clientType" value=""/>
        <classes>
            <class name="ru.vtb.app.chat.tests.Statuses"/>
            <class name="ru.vtb.app.chat.tests.Statuses"/>
            <!--            <class name="ru.vtb.app.chat.tests.Csi"/>-->
            <class name="ru.vtb.app.chat.tests.EmojiTest"/>
            <class name="ru.vtb.app.chat.tests.ConnectionTest"/>
            <class name="ru.vtb.app.chat.tests.TextSupport"/>
            <class name="ru.vtb.app.chat.tests.BackgroundTest"/>
            <class name="ru.vtb.app.chat.tests.ImagesTest"/>
            <class name="ru.vtb.app.chat.tests.QuotationTest"/>
            <class name="ru.vtb.app.chat.tests.FastButtonsTest_Hi"/>
            <class name="ru.vtb.app.chat.tests.FastButtonsTest_Text"/>
            <class name="ru.vtb.app.chat.tests.SearchTest"/>
            <class name="ru.vtb.app.chat.tests.FastButtonsTest_Any"/>
        </classes>
    </test>

