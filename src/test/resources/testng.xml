    <test name="Легкий банк" enabled="false">
        <parameter name="authStyle" value="Легкий банк"/>
        <parameter name="clientType" value=""/>
        <classes>
            <class name="ru.vtb.app.chat.tests.light.LightClientCopy"/>
            <class name="ru.vtb.app.chat.tests.light.LightClientCsi"/>
            <class name="ru.vtb.app.chat.tests.light.LightClientText_Keyboard"/>
