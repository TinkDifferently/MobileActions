<page>
    <bind>Карты</bind>
    <name>CardsPage</name>
    <elements>
        <element androidPath=""
                 iosPath="**/XCUIElementTypeWindow/XCUIElementTypeOther[2]/XCUIElementTypeOther[2]/XCUIElementTypeOther/XCUIElementTypeOther/XCUIElementTypeOther/XCUIElementTypeCollectionView"
                 type="Cards"
                 bind="Список карт" name=""/>
        <element androidPath="//*[@resource-id='ru.vtb.mobilebanking.android.rc:id/confirm_by_pass_code_keyboard' or @resource-id='ru.vtb.mobilebanking.android.rc:id/create_pass_code_keyboard']"
                 iosPath="**/XCUIElementTypeKeyboard"
                 type="CardPin"
                 bind="Новый пин-код" name=""/>
        <element iosPath="//*[./*/XCUIElementTypeButton[@name='1']]"
                 iosStrategy="xpath"
                 type="PinCodeController"
                 bind="Подтвердить пин-код" name=""/>
        <element androidPath=""
                 iosPath="**/XCUIElementTypeButton[`label='Установить'`]"
                 type="Button"
                 bind="Установить" name=""/>
        <element androidPath=""
                 iosPath="**/*[`value='ПИН-код'`]"
                 type="Button"
                 bind="Пин-код" name="PinCode"/>
        <element androidPath=""
                 iosPath="**/XCUIElementTypeButton[`label == 'Готово'`]"
                 type="Button"
                 bind="Готово" name="Ready"/>
    </elements>
</page>