<page>
    <bind>Firefox</bind>
    <name>Firefox</name>
    <elements>
        <element androidPath="org.mozilla.firefox:id/mozac_browser_toolbar_url_view"
                 androidStrategy="id"
                 type="Button"
                 bind="Url" name="Url"/>
        <element androidPath="org.mozilla.firefox:id/mozac_browser_toolbar_edit_url_view"
                 androidStrategy="id"
                 type="Input"
                 bind="Url редактируемый" name="Url"/>
        <element androidPath="//android.webkit.WebView//android.view.View[@content-desc][not(.//android.widget.Image)]"
                 type="Label"
                 bind="Ссылка устарела" name="LinkExpired"/>
        <element androidPath="//android.widget.EditText[@resource-id]"
                 type="Input"
                 bind="Номер телефона" name="PhoneNumber"/>
        <element androidPath="//android.widget.Button[@text='Продолжить']"
                 type="Button"
                 bind="Продолжить" name="Continue"/>
        <element androidPath="//android.widget.Button/*[@text='[ПреПрод] Помощник ВТБ Бизнес']"
                 type="Button"
                 bind="Диалог k4" name="K4Dialog"/>
        <element androidPath="//*[@resource-id='editable-message-text']"
                 type="Input"
                 bind="Сообщение для отправки" name="Message"/>
        <element androidPath="//android.widget.Button[@text='Send Message']"
                 type="Button"
                 bind="Отправить сообщение" name="Message"/>
        <element androidPath="//android.widget.Button[@text='Back']"
                 type="Button"
                 bind="Назад" name="Back"/>
    </elements>
</page>