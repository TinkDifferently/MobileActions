<page>
    <bind>Установщик дат</bind>
    <name>DayPicker</name>
    <elements>
        <element bind="Барабан" name="Input" type="DateScroller"
                 iosPath="**/*[`type ENDSWITH 'Picker'`]"
                 androidPath="//android.widget.NumberPicker"/>
        <element bind="Применить" name="Apply" type="Button"
                 iosPath="**/XCUIElementTypeToolbar/**/XCUIElementTypeButton[`name='Готово'`]"
                 androidPath="//android.widget.TextView[@resource-id='ru.mobiledimension.kbr:id/okBT']"/>
        <element bind="Отмена" name="Cancel" type="Button"
                 androidPath="//android.widget.TextView[@resource-id='ru.mobiledimension.kbr:id/cancelBT']"/>
    </elements>
</page>